

 {
   "version": 2,
   "name": "zomato-vishal-sarvaiya",
   "builds": [
     {
       "src": "./server.js",
       "use": "@vercel/node"
     }
   ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "./server.js",
       "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
       "headers": {
         "Access-Control-Allow-Origin": "*"
       }
     }
   ]
 }
 
 